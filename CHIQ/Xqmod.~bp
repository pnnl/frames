VERSION = BCB.03
# ---------------------------------------------------------------------------
PROJECT = Xqmod.exe
OBJFILES = Xqmod.obj "..\Common Files\gid.obj" "..\Common Files\Fcsv.obj" \
  "..\Common Files\robust.obj" "..\Common Files\ERROR.obj"
RESFILES = Xqmod.res
RESDEPEN = $(RESFILES)
LIBFILES = "..\Common Files\frames.lib"
LIBRARIES = tee35.lib vcl35.lib
SPARELIBS = vcl35.lib tee35.lib
PACKAGES = vclx35.bpi VCL35.bpi vcldb35.bpi vcldbx35.bpi dclocx35.bpi Qrpt35.bpi \
  bcbsmp35.bpi
DEFFILE =
# ---------------------------------------------------------------------------
PATHCPP = .;..\Common Files
PATHASM = .;
PATHPAS = .;
PATHRC = .;
DEBUGLIBPATH = $(BCB)\lib\debug
RELEASELIBPATH = $(BCB)\lib\release
# ---------------------------------------------------------------------------
CFLAG1 = -O2 -w -a1 -k- -vi -c -w-par -w-inl -N
CFLAG2 = -D_NO_VCL;BORLAND -I..\..\source;"..\common files";$(BCB)\include
CFLAG3 = -Tkh30000
PFLAGS = -D_NO_VCL;BORLAND -U..\..\source;"..\common files";$(BCB)\lib;$(DEBUGLIBPATH) \
  -I..\..\source;"..\common files";$(BCB)\include -$L- -$D- -v -JPHN -M
AFLAGS = /i..\..\source /i"..\common files" /i$(BCB)\include /d_NO_VCL /dBORLAND /mx /w2 \
  /zn
RFLAGS = -D_NO_VCL;BORLAND -i..\..\source;"..\common files";$(BCB)\include
LFLAGS = -L..\..\source;"..\common files";$(BCB)\lib;$(DEBUGLIBPATH) -ap -Tpe -x -Gn -c
IFLAGS =
# ---------------------------------------------------------------------------
ALLOBJ = c0x32.obj $(OBJFILES)
ALLRES = $(RESFILES)
ALLLIB = $(LIBFILES)  import32.lib cw32mt.lib
# ---------------------------------------------------------------------------
