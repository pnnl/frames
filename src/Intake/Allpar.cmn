C
C     Date         Who  Modification Description
C     --------     ---  ------------------------------------------------------
C  10-Jun-97       DLS  Initial Preparation  
C  16-Sept-2002    BAN  Added FRINH                       
C==== COMMON Block Definition ================================================
C
      COMMON/ALLPAR/UEXAIR(6),  TEXAIR(6),  UEXGRD(6),  TEXGRD(6),
     .              SHIN,       SHOUT,      FTIN(6),    FTOUT(6),
     .              EVSWIM(6),  TESWIM(6),  TSWIM(6),   SFBOAT,
     .              EVBOAT(6),  TEBOAT(6),  TBOAT(6),   EVSHOR(6),
     .              TESHOR(6),  TSHOR(6),   SWFAC,      UCRP(4,6),
     .              TCRP(4,6),  UANM(4,6),  TANM(4,6),  UAQU(4,6),
     .              TAQU(4,6),  UDW(6),     TDW(6),     USWIM(6),
     .              USHIN(6),   TESHWR(6),  EVSHWR(6),  TSHWR(6),
     .              USOIL(6),   TSOIL(6),   UINH(6),    TINH(6),
     .              UINHR(6),   FRINHR(6),  TINHR(6),   UINDRH(6),
     .              FRINDR(6),  TINDRH(6),  ASKINSH(6), ASKINSW(6),
     .              ADHSOL(6),  ASKINSL(6), ADHSED(6),  ASKINSD(6),
     .              EVSOIL(6),  TSOILD(6),  EQFRAC(6),  FRINH(6)
C
C==== Variable Declarations ==================================================
C
      REAL   UEXAIR,  TEXAIR,  UEXGRD,  TEXGRD,
     .       SHIN,    SHOUT,   FTIN,    FTOUT,
     .       EVSWIM,  TESWIM,  TSWIM,   SFBOAT,
     .       EVBOAT,  TEBOAT,  TBOAT,   EVSHOR,
     .       TESHOR,  TSHOR,   SWFAC,   UCRP,
     .       TCRP,    UANM,    TANM,    UAQU,
     .       TAQU,    UDW,     TDW,     USWIM,
     .       USHIN,   TESHWR,  EVSHWR,  TSHWR,
     .       USOIL,   TSOIL,   UINH,    TINH,
     .       UINHR,   FRINHR,  TINHR,   UINDRH,
     .       FRINDR,  TINDRH,  ASKINSH, ASKINSW,
     .       ADHSOL,  ASKINSL, ADHSED,  ASKINSD,
     .       EVSOIL,  TSOILD,  EQFRAC,  FRINH
C
