VERSION = BCB.03
# ---------------------------------------------------------------------------
PROJECT = plus.exe
OBJFILES = plus.obj "..\Common Files\robust.obj" "..\Common Files\csv.obj" \
  "..\Common Files\fcsv.obj" "..\Common Files\series.obj" \
  "..\Frames DLL\wffClass.obj" "..\Frames DLL\wcfClass.obj" \
  "..\Frames DLL\atoClass.obj" "..\Frames DLL\scfClass.obj" gid.obj wffPlus.obj \
  wcfPlus.obj atoPlus.obj scfPlus.obj plusop.obj
RESFILES = plus.res
RESDEPEN = $(RESFILES)
LIBFILES =
LIBRARIES = vcl35.lib tee35.lib
SPARELIBS = tee35.lib vcl35.lib
PACKAGES = vclx35.bpi VCL35.bpi vcldb35.bpi vcldbx35.bpi bcbsmp35.bpi dclocx35.bpi \
  Qrpt35.bpi teeui35.bpi teedb35.bpi tee35.bpi ibsmp35.bpi NMFast35.bpi \
  inetdb35.bpi inet35.bpi
DEFFILE =
# ---------------------------------------------------------------------------
PATHCPP = .;..\Common Files;..\Frames DLL
PATHASM = .;
PATHPAS = .;
PATHRC = .;
DEBUGLIBPATH = $(BCB)\lib\debug
RELEASELIBPATH = $(BCB)\lib\release
# ---------------------------------------------------------------------------
CFLAG1 = -Od -w -k- -vi -c -tWC
CFLAG2 = -D_NO_VCL;BORLAND -I"..\frames dll";"..\common files";$(BCB)\include
CFLAG3 = -Tkh30000
PFLAGS = -D_NO_VCL;BORLAND \
  -U"..\frames dll";"..\common files";$(BCB)\lib;$(DEBUGLIBPATH) -I"..\frames dll";"..\common files";$(BCB)\include \
  -$L- -$D- -v -JPHN -M
RFLAGS = -D_NO_VCL;BORLAND -i"..\frames dll";"..\common files";$(BCB)\include
AFLAGS = /i"..\frames dll" /i"..\common files" /i$(BCB)\include /d_NO_VCL /dBORLAND /mx \
  /w2 /zn
LFLAGS = -L"..\frames dll";"..\common files";$(BCB)\lib;$(DEBUGLIBPATH) -ap -Tpe -x -Gn
IFLAGS =
# ---------------------------------------------------------------------------
ALLOBJ = c0x32.obj $(OBJFILES)
ALLRES = $(RESFILES)
ALLLIB = $(LIBFILES)  import32.lib cw32mt.lib
# ---------------------------------------------------------------------------
